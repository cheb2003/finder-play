# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index
GET     /hello/:name                controllers.Application.hello(name:String)
GET     /inc                        controllers.Application.inc
GET     /incDD                      controllers.Application.incDD
GET		/indexInfo					controllers.Application.indexInfo
GET		/query 						controllers.Application.query
GET     /queryKeyword				controllers.Application.queryKeyword
POST    /queryKeyword				controllers.Application.queryKeyword
GET    	/generateSpellCheck			controllers.Application.generateSpellCheck
GET     /mkt/searchOrder/:year/:month/:day           controllers.Mkt.searchOrder(year:String,month:String,day:String)
GET     /mkt/createOrder/:year/:month/:day           controllers.Mkt.createOrder(year:String,month:String,day:String)
GET     /mkt/deleteOrder/:year/:month/:day           controllers.Mkt.deleteOrder(year:String,month:String,day:String)
GET     /keyword/searchPerDay/:year/:month/:day      controllers.KeyWord.searchPerDay(year:String,month:String,day:String)
GET     /keyword/createPerDay/:year/:month/:day      controllers.KeyWord.createPerDay(year:String,month:String,day:String)
GET     /keyword/deletePerDay/:year/:month/:day      controllers.KeyWord.deletePerDay(year:String,month:String,day:String)
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
